{
  "version": "2.0.0",
  "intentions": [],
  "entities": [],
  "dialogs": [
    {
      "name": "onboarding",
      "actions": [
        {
          "action": "call_integration",
          "integration": "my_integration",
          "function": "get_rooms",
          "function_params": []
        },
        {
          "action": "send_carousel",
          "content_source": "$my_integration.get_rooms",
          "card_content": {
            "title": "{{$room_name}}",
            "description": "{{$room_email}}",
            "image_url": "{{$room_picture}}",
            "buttons": [
              {
                "label": "Wizeline",
                "payload": "http://www.wizeline.com",
                "type": "open_url"
              },
              {
                "label": "Select Option",
                "payload": {
                  "action": "postback text"
                },
                "type": "postback"
              }
            ]
          }
        },
        {
          "action": "call_integration",
          "integration": "your_integration",
          "function": "get_other_rooms",
          "function_params": []
        },
        {
          "action": "send_carousel",
          "content_source": "$your_integration.get_other_rooms",
          "card_content": {
            "title": "{{$room_name}}",
            "description": "{{$room_email}}",
            "image_url": "{{$room_picture}}",
            "buttons": [
              {
                "label": "Wizeline",
                "payload": "http://www.wizeline.com",
                "type": "open_url"
              },
              {
                "label": "Select Option",
                "payload": {
                  "action": "postback text"
                },
                "type": "postback"
              }
            ]
          }
        }
      ]
    }
  ],
  "qa": []
}
